{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "esnext",
    "target": "es5",
    "lib": [
      "ES2015", "ES6", "dom", "esnext"
    ],
    "allowJs": true,
    "sourceMap": true,
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
  },
  "compileOnSave": true,
}